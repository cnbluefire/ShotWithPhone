<Application
    x:Class="ShotWithPhone.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="using:ShotWithPhone"
    xmlns:vm="using:ShotWithPhone.ViewModel"
    xmlns:model="using:ShotWithPhone.Model"
    RequestedTheme="Light">
    <Application.Resources>
        <vm:ViewModelLocator x:Key="Locator" />
        <SolidColorBrush Color="{ThemeResource SystemAccentColor}" x:Key="SAC" />
        <model:AppSettings x:Key="AppSettings" />
    </Application.Resources>
</Application>
